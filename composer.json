{
  "name": "spryker/graphviz",
  "type": "library",
  "description": "Graphviz module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "phpdocumentor/graphviz": "^1.0.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
